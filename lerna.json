{
  "useWorkspaces": true,
  "version": "independent",
  "npmClient": "yarn",
  "command": {
    "version": {
      "createRelease": "github",
      "conventionalCommits": true,
      "allowBranch": ["master","alpha","beta","next","next-major","next-minor","next-patch"],
      "ignoreChanges": ["**/*.md", "**/package.json"],
      "message": "chore(release): Publish Images"
    }
  }
}
