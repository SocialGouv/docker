    "bats-assert": "bats-core/bats-assert#2.0.0",
    "bats-file": "bats-core/bats-file#0.3.0",
    "bats-support": "bats-core/bats-support#0.3.0",
    "husky": "^8.0.0",
    "lerna": "^6.6.2"
  },
  "scripts": {
    "postversion": "./.scripts/push-tags-one-by-one",
